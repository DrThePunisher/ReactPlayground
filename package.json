{
  "name": "playground",
  "version": "0.1.0",
  "private": true,
  "dependencies": {},
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "watch-css": "node-sass-chokidar src/ -o src/ --include-path src/assets/styles/ --watch --recursive",
    "start-js": "react-scripts-ts start",
    "build": "npm-run-all build-css build-js",
    "build-js": "react-scripts-ts build",
    "build-css": "node-sass-chokidar src/ -o src/ --include-path src/assets/styles/",
    "test": "mocha ./src/**/*.test.*",
    "eject": "react-scripts-ts eject",
    "db": "json-server --watch src/db/db.json --port 3004"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.1.3",
    "@types/chai-enzyme": "^0.6.4",
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jsdom": "^11.0.4",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.0.8",
    "@types/prop-types": "^15.5.3",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/sinon": "^4.3.3",
    "@types/sinon-chai": "^2.7.31",
    "axios": "^0.18.0",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.0",
    "choma": "^1.2.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.10.0",
    "json-server": "^0.12.2",
    "mocha": "^5.1.1",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts-ts": "2.15.1",
    "sinon": "^5.0.7",
    "sinon-chai": "^3.0.0",
    "ts-node": "^6.0.3",
    "typescript": "^2.8.3"
  }
}
